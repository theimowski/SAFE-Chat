source https://www.nuget.org/api/v2
storage: none

nuget Suave
nuget Suave.OAuth
nuget Suave.Experimental
nuget Akkling
nuget Akkling.Streams
nuget Akkling.Persistence
nuget Akka.Persistence.SqlServer
nuget Akka.Persistence.Sqlite

nuget Newtonsoft.Json
nuget Fable.JsonConverter

nuget Microsoft.Extensions.Configuration
nuget Microsoft.Extensions.Configuration.Json

nuget Fable.Elmish.HMR
nuget FSharp.Core
nuget Fable.Core
nuget Fable.Elmish.React
nuget Fable.Elmish.Browser
nuget Fable.Elmish.Debugger
nuget Fable.Websockets.Elmish

clitool dotnet-fable

group UITests
  source https://nuget.org/api/v2
  framework >= net461
  nuget FSharp.Core redirects: force
  nuget Canopy
  nuget Expecto
  nuget phantomjs
  nuget Selenium.WebDriver.ChromeDriver

group Build
    storage: none
    source https://api.nuget.org/v3/index.json
    source ~/oss/SAFE-template/nupkg

    nuget SAFE.Build
